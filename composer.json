{
  "name": "senseexception/dumpedie",
  "description": "Another var_dump-like debugging function with special debugging features",
  "minimum-stability": "dev",
  "type": "library",
  "license": "MIT",
  "keywords": ["dump", "var_dump", "debugging", "debug"],
  "authors": [
    {
      "name": "Claudio Zizza",
      "email": "claudio@budgegeria.de"
    }
  ],
  "require": {
    "php": "^7.1",
    "symfony/var-dumper": "^3.4|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "files": [ "src/functions.php" ],
    "psr-4": {
      "Budgegeria\\Dumpedie\\": "src/"
    },
    "exclude-from-classmap": ["tests/*"]
  },
  "autoload-dev": {
    "psr-4": {
      "Budgegeria\\Dumpedie\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true
}
